# Example environment variables for BackLogus production
# Copy to .env and fill in your real values

# Database
DATABASE_URL=postgresql://user:password@db:5432/backlogus_db

# Auth
JWT_SECRET=your-super-secret-jwt-key

# IGDB API (games)
IGDB_CLIENT_ID=your-igdb-client-id
IGDB_ACCESS_TOKEN=your-igdb-access-token

# TMDB API (movies/TV)
TMDB_API_KEY=your-tmdb-api-key
